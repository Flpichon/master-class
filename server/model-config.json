{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "../node_modules/loopback-ds-timestamp-mixin",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "accesstoken": {
    "dataSource": "db",
    "public": true
  },
  "acl": {
    "dataSource": "db",
    "public": false
  },
  "rolemapping": {
    "dataSource": "db",
    "public": false
  },
  "role": {
    "dataSource": "db",
    "public": false
  },
  "user": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "create": true,
          "login": true,
          "logout": true,
          "find": true,
          "prototype.__get__accessTokens": true,
          "prototype.__destroyById__accessTokens": true,
          "prototype.__findById__accessTokens": true,
          "prototype.__get__userRoles": true,
          "prototype.__create__userRoles": true,
          "prototype.isValidToken": true,
          "prototype.__create__conference": true,
          "prototype.__create__project": true
        }
      }
    }
  },
  "document": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "find": true,
          "create": true
        }
      }
    }
  },
  "userrole": {
    "dataSource": "db",
    "public": true
  },
  "conference": {
    "dataSource": "db",
    "public": true
  },
  "project": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "prototype.__create__users": true,
          "prototype.findAPiKey": true,
          "prototype.getAllConferences": true
        }
      }
    }
  },
  "container": {
    "dataSource": "stockage",
    "public": true
  },
  "article": {
    "dataSource": "db",
    "public": true
  }
}
