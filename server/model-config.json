{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "AccessToken": {
    "dataSource": "masterclass",
    "public": true,
    "relations": {
      "user": {
        "type": "belongsTo",
        "model": "user",
        "foreignKey": "userId"
      }
    }
  },
  "ACL": {
    "dataSource": "masterclass",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "masterclass",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "masterclass",
    "public": false
  },
  "user": {
    "dataSource": "masterclass",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "create": true,
          "login": true,
          "logout": true,
          "find": true,
          "prototype.__get__accessTokens": true,
          "prototype.__destroyById__accessTokens": true,
          "prototype.__findById__accessTokens": true,
          "prototype.__get__userRoles": true,
          "prototype.__create__userRoles": true,
          "prototype.isValidToken": true,
          "prototype.__create__conference": true
        }
      }
    }
  },
  "document": {
    "dataSource": "masterclass",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "find": true,
          "create": true
        }
      }
    }
  },
  "userRole": {
    "dataSource": "masterclass",
    "public": true
  },

  "conference": {
    "dataSource": "masterclass",
    "public": true
  }
}
