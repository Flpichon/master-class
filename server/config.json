{
  "restApiRoot": "/api",
  "host": "masterofirefrost.mysql.db",
  "port": 3306,
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "sharedMethods": {
      "*": true
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  },
  "logoutSessionsOnSensitiveChanges": true
}
